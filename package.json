{
  "name": "react-pkg-placeholder",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/react-pkg-placeholder.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/react-pkg-placeholder.js"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview --port 4173",
    "lint": "eslint src/ --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --fix src/ --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "lint-staged": {
    "*.js(?:on)?": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "pnpm lint:fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.9"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "12.1.2",
    "@types/lodash-es": "4.17.10",
    "@types/node": "14.18.9",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "5.1.29",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@vitejs/plugin-react": "3.1.0",
    "@vitejs/plugin-react-swc": "3.3.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-define-config": "1.11.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "postcss": "8.4.23",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "0.16.0"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
